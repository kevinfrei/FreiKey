{
  "env": {
    "gccRoot": "/usr/local/opt/gcc-arm-none-eabi",
    "gccVer": "8.3.2",
    "srcRoot": "${workspaceRoot}",
    "variableReference": "https://code.visualstudio.com/docs/editor/variables-reference",
    "fileReference": "https://github.com/Microsoft/vscode-cpptools/blob/master/Documentation/LanguageServer/c_cpp_properties.json.md"
  },
  "configurations": [
    {
      "name": "FreiKeys",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "compilerPath": "${gccRoot}/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceRoot}/out-freikeys/compile_commands.json",
      "defines": ["FREIKEYS", "BTLE_HOST", "DEBUG=2"],
      "includePath": [
        "${workspaceRoot}/include",
        "${workspaceRoot}/include/freikeys",
        "${workspaceRoot}/include/adafruit_nrf52"
      ]
    },
    {
      "name": "Karbon",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "compilerPath": "${gccRoot}/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceRoot}/out/karbon/compile_commands.json",
      "defines": ["KARBON", "BTLE_HOST", "MACRO_PAD", "HAS_DISPLAY", "DEBUG=2"],
      "includePath": [
        "${workspaceRoot}/include",
        "${workspaceRoot}/include/karbon",
        "${workspaceRoot}/include/adafruit_nrf52"
      ]
    },
    {
      "name": "Karbon-Left",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
