      "cStandard": "c11",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceRoot}/out/l-karbon/compile_commands.json"
    },
    {
      "name": "Karbon-Right",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "compilerPath": "${gccRoot}/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceRoot}/out/r-karbon/compile_commands.json"
    },
    {
      "name": "Mock",
      "intelliSenseMode": "clang-x64",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceRoot}/out-mock-master/compile_commands.json"
    },
    {
      "name": "Left FreiKeys",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
