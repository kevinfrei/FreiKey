    },
    {
      "name": "Right FreiKeys",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "compilerPath": "${gccRoot}/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "compileCommands": "${workspaceRoot}/out-rfreikeys/compile_commands.json"
