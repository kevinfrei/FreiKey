{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build",
      "type": "shell",
      "command": "make -j",
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Clean",
      "type": "shell",
      "command": "make allclean",
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Flash Right",
      "type": "shell",
      "command": "make flashr",
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Flash Left",
      "type": "shell",
      "command": "make flashl",
      "problemMatcher": ["$gcc"]
    }
  ]
}
